{
  "name": "@thisisagile/easy",
  "version": "0.1.0",
  "description": "Straightforward library for building domain-driven microservice architectures",
  "author": "Sander Hoogendoorn",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:thisisagile/easy.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./data/*": {
      "import": "./dist/data/*.mjs",
      "types": "./dist/data/*.d.ts"
    },
    "./domain/*": {
      "import": "./dist/domain/*.mjs",
      "types": "./dist/domain/*.d.ts"
    },
    "./heatlh/*": {
      "import": "./dist/heatlh/*.mjs",
      "types": "./dist/heatlh/*.d.ts"
    },
    "./http/*": {
      "import": "./dist/http/*.mjs",
      "types": "./dist/http/*.d.ts"
    },
    "./process/*": {
      "import": "./dist/process/*.mjs",
      "types": "./dist/process/*.d.ts"
    },
    "./resources/*": {
      "import": "./dist/resources/*.mjs",
      "types": "./dist/resources/*.d.ts"
    },
    "./security/*": {
      "import": "./dist/security/*.mjs",
      "types": "./dist/security/*.d.ts"
    },
    "./services/*": {
      "import": "./dist/services/*.mjs",
      "types": "./dist/services/*.d.ts"
    },
    "./sql/*": {
      "import": "./dist/sql/*.mjs",
      "types": "./dist/sql/*.d.ts"
    },
    "./types/*": {
      "import": "./dist/types/*.mjs",
      "types": "./dist/types/*.d.ts"
    },
    "./utils/*": {
      "import": "./dist/utils/*.mjs",
      "types": "./dist/utils/*.d.ts"
    },
    "./validation/*": {
      "import": "./dist/validation/*.mjs",
      "types": "./dist/validation/*.d.ts"
    }
  },
  "keywords": [
    "typescript",
    "microservices",
    "domain-driven-design",
    "evolutionary-architecture"
  ],
  "scripts": {
    "lint": "yarn g:eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "yarn g:prettier --check --write src test *.json",
    "build": "yarn g:tsc ",
    "build:tsup": "yarn g:tsup --onSuccess \"yarn g:tsc --emitDeclarationOnly --declaration\"",
    "test": "yarn g:jest --coverage",
    "prepack": "yarn g:copy-readme"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@thisisagile/easy-test": "workspace:*",
    "@types/form-urlencoded": "^4.4.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/luxon": "3.4.2",
    "@types/validator": "^13.11.10"
  },
  "dependencies": {
    "@types/uuid": "^9.0.8",
    "axios": "^1.3.4",
    "form-urlencoded": "^6.1.0",
    "jsonwebtoken": "^9.0.0",
    "luxon": "^3.4.4",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.1",
    "validator": "^13.11.0"
  }
}
